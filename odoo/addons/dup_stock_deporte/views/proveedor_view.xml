<odoo>
    <record id="view_proveedor_form" model="ir.ui.view">
        <field name="name">proveedor.form</field>
        <field name="model">dup_stock_deporte.proveedor</field>
        <field name="arch" type="xml">
            <form string="Proveedor">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="direccion"/>
                        <field name="telefono"/>
                        <field name="producto_ids" widget="one2many_list"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_proveedor_tree" model="ir.ui.view">
        <field name="name">proveedor.tree</field>
        <field name="model">dup_stock_deporte.proveedor</field>
        <field name="arch" type="xml">
            <tree string="Proveedores">
                <field name="name"/>
                <field name="direccion"/>
                <field name="telefono"/>
            </tree>
        </field>
    </record>
</odoo>
